main:
	ch = 72
	t2 = addrof ch
	t1 = t2 + 1
	write t1, 101
	t4 = addrof ch
	t3 = t4 + 2
	write t3, 108
	t6 = addrof ch
	t5 = t6 + 3
	write t5, 111
	t8 = addrof ch
	t7 = t8 + 4
	write t7, 0
	t9 = addrof ch
	call draw_line(0, t9, 0, 3)
	ret
draw_line:
	t9 = t1 * 32
	t8 = video_ram + t9
	t5 = t8
	t10 = t3 | t4
	t6 = t10
	t11 = read t2
	je 8, t11, 0
	t13 = read t2
	t12 = t6 | t13
	write t5, t12
	t7 = t2
	t14 = t2 + 1
	t2 = t14
	jmp -8
	ret
